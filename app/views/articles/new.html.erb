<h1>Articles#new</h1>
<p>Find me in app/views/articles/new.html.erb</p>

<%= simple_form_for @article do |f| %>
  <%= f.input :url %>
  <%= f.input :title %>
  <%= f.input :importance %>
  <%= f.input :reminder %>
  <%= f.input :note %>
  <%= hidden_field_tag :folder, params[:folder_id_for_new_article] %>
  <%= f.button :submit %>
<% end %>
